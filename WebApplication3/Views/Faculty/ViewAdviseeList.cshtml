@model List<WebApplication3.HelperClasses.Advisee>
@{
    ViewBag.Title = "ViewAdviseeList";
    Layout = "~/Views/Shared/_LayoutFaculty.cshtml";
}

<link rel="stylesheet" href="~/Content/TableTemplate.css" />
<table class="resultsTable" id="lines">
    <tr class="tableheader">
        <th>Student ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        <th>DOB</th>
        <th>Street</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th
    </tr>
    @foreach (var v in Model)
    {
    <tr>
        <td>@v.studentID</td>
        <td>@v.firstName</td>
        <td>@v.lastName</td>
        <td>@v.phone</td>
        <td>@v.dob</td>
        <td>@v.street</td>
        <td>@v.city</td>
        <td>@v.state</td>
        <td>@v.zip</td>
    </tr>
    }
</table>


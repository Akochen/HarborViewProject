@model List<WebApplication3.HelperClasses.StudentEnrollment>
<link rel="stylesheet" href="~/Content/TableTemplate.css" />
@{
    ViewBag.Title = "ViewEnrollees";
    Layout = "~/Views/Shared/_LayoutFaculty.cshtml";
}
<table class="resultsTable" id="lines">
    <tr class="tableheader">
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Date of Birth</th>
        <th></th>
    </tr>
    @foreach (var v in Model)
    {
    <tr>
        <td>@v.firstName</td>
        <td>@v.lastName</td>
        <td>@v.email</td>
        <td>@v.phoneNumber</td>
        <td>@v.dob</td>
        <td>@Html.ActionLink("Assign Final Grade", "AssignGradeEnrollees", "Faculty", new { sectionID = v.sectionID, year = v.year, semester = v.semester,studentID = v.studentID,firstName = v.firstName,lastName = v.lastName, courseName = v.courseName,v.credits}, null)</td>
    </tr>
    }
</table>




@model List<WebApplication3.HelperClasses.Hold>
<link rel="stylesheet" href="~/Content/TableTemplate.css" />
@{
    Layout = null;
}

<head>
</head>
<body>

    <link rel="stylesheet" href="~/Content/hold.css" />
    <table class="holds">
        <tr class="tableheader">
            <th>Type</th>
            <th>Semester</th>
            <th>Year</th>
            <th></th>
        </tr>
        @foreach (var v in Model)
        {
            <tr>
                <td>@v.holdType </td>
                <td>@v.semester</td>
                <td>@v.year</td>
                <td>@Html.ActionLink("Remove Hold", "removeHold", "Administrator", new {studentID = TempData["studentID"],year = v.year, semester = v.semester, holdType = v.holdType}, null)</td>
            </tr>
        }
    </table>
</body>


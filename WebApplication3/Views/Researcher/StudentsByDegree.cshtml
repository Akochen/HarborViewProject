@model List<WebApplication3.HelperClasses.ResearcherMajorData>
@{
    Layout = null;
}

<link rel="stylesheet" href="~/Content/TableTemplate.css" />
@if (Model.Count > 0)
{
    <table class="resultsTable">
        <tr class="tableheader">
            <th>Major Name</th>
            <th>Count</th>
        </tr>
        @foreach (var v in Model)
        {
            <tr>
                <td>@v.majorName</td>
                <td>@v.studentCount</td>
            </tr>
        }
    </table>
}
else
{
    <strong>No results found</strong>
}